<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Main Window" Height="450" Width="800">
    <Grid>
        <!-- Other UI elements -->
        <Button Name="AddProductButton" Content="Add Product" Click="AddProductButton_Click" Width="200" Height="30" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10"/>
        <Button Name="ViewProductsButton" Content="View Products" Click="ViewProductsButton_Click" Width="200" Height="30" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,50"/>
        <Button Name="ViewCartButton" Content="View Cart" Click="ViewCartButton_Click" Width="200" Height="30" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,90"/>
        <Button x:Name="AdminButton" Content="Admin panel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Height="30" Click="AdminButton_Click"  Margin="10,130"/>

    </Grid>
</Window>
